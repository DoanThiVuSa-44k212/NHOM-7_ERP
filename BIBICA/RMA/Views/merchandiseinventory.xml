<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="merchandiseinventory_action" model="ir.actions.act_window">
        <field name="name">Merchandise Inventory</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">rma.merchandiseinventory</field>
        <field name="view_mode">tree,kanban,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first merchandiseinventory!
            </p>
        </field>
    </record>


    <menuitem id="rma_root"
            name="RMA"
            sequence="10"/>

    <menuitem id="rma_merchandiseinventory_root"
            name="Merchandise Inventory"
            parent="rma_root"
            sequence="10"/>

    <menuitem id="rma_merchandiseinventory"
            name="Merchandise Inventory"
            parent="rma_merchandiseinventory_root"
            action="merchandiseinventory_action"
              sequence="10"/>

</odoo>