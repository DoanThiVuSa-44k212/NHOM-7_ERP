<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="productpreserve_action" model="ir.actions.act_window">
        <field name="name">Product Preserve</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">rma.productpreserve</field>
        <field name="view_mode">tree,kanban,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first productpreserve!
            </p>
        </field>
    </record>


    <menuitem id="rma_root"
            name="RMA"
            sequence="10"/>

    <menuitem id="rma_productpreserve_root"
            name="Product Preserve"
            parent="rma_root"
            sequence="10"/>

    <menuitem id="rma_productpreserve"
            name="Product Preserve"
            parent="rma_productpreserve_root"
            action="productpreserve_action"
              sequence="10"/>

</odoo>