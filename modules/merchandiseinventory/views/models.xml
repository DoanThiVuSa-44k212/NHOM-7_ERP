<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="merchandise_inventory_act" model="ir.actions.act_window">
            <field name="name">Merchandise_Information</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">merchandise_inventory.merchandise_inventory</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Tạo phiếu kiểm kê đầu tiên của bạn
              </p>
            </field>
        </record>
        <record id="Merchandise_Inventory_tree" model="ir.ui.view">
            <field name="name">Merchandise_Information</field>
            <field name="model">merchandise_inventory.merchandise_inventory</field>
            <field name="arch" type="xml">
                <tree string="Mon hoc ">
                    <field name="name"/>
                    <field name="type"/>
                    <field name="quantity1"/>
                    <field name="quantity2"/>
                    <field name="quantity3"/>
                    <field name="quantity4"/>
                    <field name="Fact "/>
                    <field name="difference"/>
                    <field name="date"/>
                    <field name="description"/>

                </tree>
            </field>
        </record>
          <menuitem id="merchandise_inventory_root"
              name="MerchandiseInventory"
              sequence="10"/>
    <menuitem id="merchandise_inventory_goods_root"
              name="MyMerchandiseInventory"
              parent="merchandise_inventory_root"
              sequence="10"/>
    <menuitem id="merchandise_inventory_root_goods"
              name="MyMerchandiseInventory_root"
              parent="merchandise_inventory_root_goods_root"
              action="merchandise_inventory_root_act"
              sequence="10"/>
    </data>
</odoo>